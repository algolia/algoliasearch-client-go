{
  "java": {
    "folder": "clients/algoliasearch-client-java-2",
    "mainPackage": "java-search",
    "modelFolder": "algoliasearch-core/src/com/algolia/model",
    "apiFolder": "algoliasearch-core/src/com/algolia/api",
    "customGenerator": "algolia-java",
    "tests": {
      "extension": ".test.java",
      "outputFolder": "src/test/java/com/algolia"
    }
  },
  "javascript": {
    "folder": "clients/algoliasearch-client-javascript",
    "mainPackage": "javascript-search",
    "modelFolder": "model",
    "apiFolder": "src",
    "customGenerator": "algolia-javascript",
    "tests": {
      "extension": ".test.ts",
      "outputFolder": "src"
    }
  },
  "php": {
    "folder": "clients/algoliasearch-client-php",
    "mainPackage": "php-search",
    "modelFolder": "lib/Model",
    "customGenerator": "algolia-php",
    "apiFolder": "lib/Api",
    "tests": {
      "extension": "Test.php",
      "outputFolder": "src"
    }
  }
}
