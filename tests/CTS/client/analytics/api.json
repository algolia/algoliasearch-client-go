[
  {
    "testName": "calls api with correct user agent",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "getAverageClickPosition",
        "parameters": {
          "index": "my-index"
        },
        "expected": {
          "testSubject": "userAgent",
          "match": {
            "regexp": "/Algolia%20for%20(.+)%20\\(\\d+\\.\\d+\\.\\d+\\)/"
          }
        }
      }
    ]
  },
  {
    "testName": "calls api with correct timeouts",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "getAverageClickPosition",
        "parameters": {
          "index": "my-index"
        },
        "expected": {
          "match": {
            "objectContaining": {
              "connectTimeout": 2000,
              "responseTimeout": 5000
            }
          }
        }
      }
    ]
  }
]
