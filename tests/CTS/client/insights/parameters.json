[
  {
    "testName": "fallbacks to the alias when region is not given",
    "autoCreateClient": false,
    "steps": [
      {
        "type": "createClient",
        "parameters": {
          "appId": "my-app-id",
          "apiKey": "my-api-key"
        },
        "expected": {}
      },
      {
        "type": "method",
        "object": "$client",
        "path": "pushEvents",
        "parameters": {
          "events": []
        },
        "expected": {
          "type": "host",
          "match": "insights.algolia.io"
        }
      }
    ]
  }
]
