[
  {
    "testName": "calls api with correct user agent",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "fetchUserProfile",
        "parameters": {
          "userID": "user1",
          "params": {
            "modelsToRetrieve": [
              "funnel_stage"
            ]
          }
        },
        "expected": {
          "testSubject": "userAgent",
          "match": {
            "regexp": "/Algolia%20for%20(.+)%20\\(\\d+\\.\\d+\\.\\d+\\)/"
          }
        }
      }
    ]
  },
  {
    "testName": "calls api with correct timeouts",
    "steps": [
      {
        "type": "method",
        "object": "$client",
        "path": "fetchUserProfile",
        "parameters": {
          "userID": "user1",
          "params": {
            "modelsToRetrieve": [
              "funnel_stage"
            ]
          }
        },
        "expected": {
          "match": {
            "objectContaining": {
              "connectTimeout": 2,
              "responseTimeout": 30
            }
          }
        }
      }
    ]
  }
]
